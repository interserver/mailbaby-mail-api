{
  "name": "interserver/mailbaby-mail-api",
  "type": "project",
  "keywords": [
    "high performance",
    "http service"
  ],
  "homepage": "http://mail.baby",
  "license": "MIT",
  "description": "API service for accessing the Mail.Baby services.",
  "authors": [
    {
      "name": "Joe Huss",
      "email": "detain@interserver.net",
      "homepage": "https://www.interserver.net",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "detain@interserver.net",
    "issues": "https://github.com/walkor/webman/issues",
    "forum": "https://wenda.workerman.net/",
    "wiki": "https://workerman.net/doc/webman",
    "source": "https://github.com/interserver/mailbaby-mail-api"
  },
  "require": {
    "php": ">=7.2",
    "workerman/webman-framework": "*",
    "monolog/monolog": "*",
    "vlucas/phpdotenv": "*",
    "psr/container": "*",
    "jenssegers/mongodb": "*",
    "illuminate/database": "*",
    "illuminate/redis": "*",
    "illuminate/events": "*",
    "symfony/var-dumper": "*",
    "phpmailer/phpmailer": "dev-master",
    "phpmailer/dkimvalidator": "dev-master",
    "php-mime-mail-parser/php-mime-mail-parser": "*",
    "respect/validation": "*",
    "webman/console": "*",
    "webman/cors": "*",
    "webman/database": "*"
  },
  "suggest": {
    "ext-event": "For better performance. "
  },
  "autoload": {
    "psr-4": {
      "": "./",
      "app\\": "./app",
      "App\\": "./app",
      "app\\View\\Components\\": "./app/view/components"
    },
    "files": [
      "./support/helpers.php"
    ]
  },
  "scripts": {
    "post-package-install": [
      "support\\Plugin::install"
    ],
    "post-package-update": [
      "support\\Plugin::install"
    ],
    "pre-package-uninstall": [
      "support\\Plugin::uninstall"
    ]
  },
  "require-dev": {
    "league/openapi-psr7-validator": "^0.15.2"
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": false
    }
  }
}
