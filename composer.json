{
  "name": "interserver/mailbaby-mail-api",
  "type": "project",
  "keywords": [
    "high performance",
    "http service"
  ],
  "homepage": "http://mail.baby",
  "license": "MIT",
  "description": "API service for accessing the Mail.Baby services.",
  "authors": [
    {
      "name": "Joe Huss",
      "email": "detain@interserver.net",
      "homepage": "https://www.interserver.net",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "detain@interserver.net",
    "issues": "https://github.com/walkor/webman/issues",
    "forum": "https://wenda.workerman.net/",
    "wiki": "https://workerman.net/doc/webman",
    "source": "https://github.com/interserver/mailbaby-mail-api"
  },
  "require": {
    "php": ">=7.2",
    "workerman/webman-framework": "^1.4.3",
    "monolog/monolog": "^2.0",
    "vlucas/phpdotenv": "^5.4",
    "psr/container": "^1.1.1",
    "illuminate/database": "^8.83",
    "illuminate/pagination": "^8.83",
    "symfony/var-dumper": "^5.4",
    "phpmailer/phpmailer": "^6.6",
    "webman/console": "^1.2",
    "webman/cors": "^1.0"
  },
  "suggest": {
    "ext-event": "For better performance. "
  },
  "autoload": {
    "psr-4": {
      "": "./",
      "App\\": "./app"
    },
    "files": [
      "./support/helpers.php"
    ]
  },
  "scripts": {
    "post-package-install": [
      "support\\Plugin::install"
    ],
    "post-package-update": [
      "support\\Plugin::install"
    ],
    "pre-package-uninstall": [
      "support\\Plugin::uninstall"
    ]
  },
  "require-dev": {
    "league/openapi-psr7-validator": "^0.15.2"
  }
}
